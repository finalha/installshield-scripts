RemoveFeatures:	
    if  ((nResult1 !=1 && svFeatureRM = "1") || (nResult2 !=1 && svFeatureRabbitMQ = "1") || (nResult3 !=1 && svFeatureRedis = "1") || (nResult4 !=1 && svFeatureWS = "1")) then    
