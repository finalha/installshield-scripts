//---------------------------------------------------------------------------
// OnEnd
//
// The OnEnd event is called at the end of the setup. This event is not
// called if the setup is aborted.
//---------------------------------------------------------------------------
function OnEnd()
    STRING szTitle, szMsg1, szMsg2, szOpt1, szOpt2;
    NUMBER bOpt1, bOpt2;
begin
	if (hMutex) then
		CloseHandle(hMutex);
	endif;
	DeleteFolder( svMongodbinitInstalldir);
	if (!REMOVEONLY) then
	if(SYSINFO.bIsWow64) then 
    REGDB_OPTIONS = REGDB_OPTIONS | REGDB_OPTION_WOW64_64KEY;
	nvType = REGDB_STRING; 
    nvSize = -1;  
    if (RegDBSetDefaultRoot (HKEY_LOCAL_MACHINE) = 0) then
    	RegDBSetKeyValueEx(REGROOT,REGKEY_RM,nvType,svFeatureRM,nvSize);
        RegDBSetKeyValueEx(REGROOT,REGKEY_RABBITMQ,nvType,svFeatureRabbitMQ,nvSize);
        RegDBSetKeyValueEx(REGROOT,REGKEY_REDIS,nvType,svFeatureRedis,nvSize);        
        RegDBSetKeyValueEx(REGROOT,REGKEY_WS,nvType,svFeatureWS,nvSize);
        RegDBSetKeyValueEx(REGROOT,REGKEY_INITDATA,nvType,svinitdata,nvSize);
        RegDBSetKeyValueEx(REGROOT,REGKEY_UPGRADEDATA,nvType,svupgradedata,nvSize);
    endif;
    REGDB_OPTIONS = REGDB_OPTIONS & ~REGDB_OPTION_WOW64_64KEY;
    endif;
    WriteRegistryDump();
    endif;    
    
    if (REMOVEONLY) then
    DeleteRegistryDump();
    DeleteFolder(DUMP_PATH);
    endif;
end;

